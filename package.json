{
  "name": "ssrjobportal",
  "version": "1.0.0",
  "description": "server side rendering example",
  "main": "index.js",
  "scripts": {
    "build": "set NODE_ENV=development && webpack -d",
    "start": "set NODE_ENV=development && npm run build && nodemon --ignore src/static/ --exec babel-node -- src/server/index.js",
    "build:prod": "set NODE_ENV=production && webpack -p",
    "start:prod": "set NODE_ENV=production && npm run build:prod && nodemon --ignore src/static/ --exec babel-node -- src/server/index.js",
    "test": "jest"
  },
  "keywords": [],
  "devDependencies": {
    "@babel/core": "7.6.0",
    "@babel/node": "7.6.1",
    "@babel/plugin-transform-runtime": "7.6.0",
    "@babel/preset-env": "7.6.0",
    "@babel/preset-react": "7.0.0",
    "babel-loader": "8.0.6",
    "nodemon": "1.19.2",
    "webpack": "4.40.2",
    "webpack-cli": "3.3.9"
  },
  "dependencies": {
    "@babel/runtime": "7.6.0",
    "axios": "0.21.1",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "express": "5.0.0-alpha.7",
    "jest": "^24.9.0",
    "jest-cli": "^24.9.0",
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "react-helmet": "^5.2.1",
    "react-redux": "^7.1.1",
    "react-router-dom": "^5.0.1",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0",
    "styled-components": "^4.4.0"
  }
}
